price = float(input("Enter price: "))
if price == 2:
  tax = 0.07
  print(("Tax rate is ") + str(tax))
if price == 1:
  tax = 0.07
  print(("Tax rate is ") + str(tax))
if price == 0.5:
  tax = 0
  print(("Tax rate is ") + str(tax))
if price > 1:
  tax = 0.7
  print(("Tax rate is ") + str(tax))
else:
  tax = 0
